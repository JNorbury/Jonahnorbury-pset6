<resources>

    <!-- Base application theme. -->
    <style type="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item type="colorPrimary">@color/colorPrimary</item>
        <item type="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item type="colorAccent">@color/colorAccent</item>
    </style>

    <style type="AppTheme.NoActionBar">
        <item type="windowActionBar">false</item>
        <item type="windowNoTitle">true</item>
    </style>

    <style type="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style type="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
